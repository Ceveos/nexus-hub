{
	"name": "orange-forest-5e6d",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "npm-run-all -p dev:*",
		"dev:worker": "cloudflared-output-domain --url http://127.0.0.1:55555 -- echo DATABASE_URL=prisma://{host}/?api_key=dummy > .dev.vars && wrangler dev",
		"dev:proxy": "prisma-accelerate-local postgresql://postgres:password@localhost:5432/postgres -p 8000 -t",
		"dev:cloudflared": "cloudflared --metrics 127.0.0.1:55555 --url http://127.0.0.1:8000",
		"prisma:generate": "prisma generate --schema ../prisma/schema.prisma",
		"prisma:generate-prod": "prisma generate --no-engine --schema ../prisma/schema.prisma",
		"deploy": "pnpm run prisma:generate-prod && wrangler deploy"
	},
	"dependencies": {
		"drizzle-orm": "^0.29.3",
		"postgres": "^3.4.3"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20231218.0",
		"cloudflared": "^0.5.0",
		"cloudflared-output-domain": "^0.0.2",
		"drizzle-kit": "^0.20.13",
		"npm-run-all": "^4.1.5",
		"shared": "workspace:*",
		"typescript": "^5.0.4",
		"wrangler": "^3.0.0"
	}
}
